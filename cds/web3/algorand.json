{
    "key": "near",
    "name": "Near blockchain",
    "version": "1.0.0",
    "platformVersion": "1.0.0",
    "triggers": [
        {
            "key": "newTransaction",
            "name": "Wallet transfer",
            "display": {
                "label": "Wallet transfer",
                "description": ""
            },
            "operation": {
                "type": "polling",
                "operation": {
                    "url": "wss://gnexus-connector-web3.herokuapp.com/"
                },
                "inputFields": [
                    {
                        "key": "_grinderyChain",
                        "type": "string",
                        "computed": true,
                        "default": "algorand:mainnet"
                    },
                    {
                        "key": "from",
                        "label": "From",
                        "type": "address",
                        "placeholder": "Enter a Algorand wallet address",
                        "list": false
                    },
                    {
                        "key": "to",
                        "label": "To",
                        "type": "address",
                        "placeholder": "Enter a Algorand wallet address",
                        "list": false
                    }
                ],
                "outputFields": [
                    {
                        "key": "from",
                        "label": "From",
                        "type": "address",
                        "placeholder": "Enter a Algorand wallet address",
                        "list": false
                    },
                    {
                        "key": "to",
                        "label": "To",
                        "type": "address",
                        "placeholder": "Enter a Algorand wallet address",
                        "list": false
                    },
                    {
                        "key": "amount",
                        "label": "Amount",
                        "type": "string",
                        "placeholder": "",
                        "list": false
                    }
                ],
                "sample": {}
            }
        },
        {
            "key": "TokenTransferTrigger",
            "name": "Token transfer",
            "display": {
                "label": "Token transfer",
                "description": ""
            },
            "operation": {
                "type": "blockchain:event",
                "signature": "axfer",
                "inputFields": [
                    {
                        "key": "_grinderyChain",
                        "type": "string",
                        "computed": true,
                        "default": "algorand:mainnet"
                    },
                    {
                        "key": "_grinderyContractAddress",
                        "type": "string",
                        "label": "Token ID",
                        "placeholder": "Enter Token ID (0x0 for any)",
                        "default": ""
                    },
                    {
                        "key": "snd",
                        "label": "From",
                        "type": "address",
                        "placeholder": "Enter a Algorand wallet address",
                        "list": false
                    },
                    {
                        "key": "arcv",
                        "label": "To",
                        "type": "address",
                        "placeholder": "Enter a Algorand wallet address",
                        "list": false
                    }
                ],
                "outputFields": [
                    {
                        "key": "snd",
                        "label": "From",
                        "type": "address",
                        "placeholder": "Enter a Algorand wallet address",
                        "list": false
                    },
                    {
                        "key": "arcv",
                        "label": "To",
                        "type": "address",
                        "placeholder": "Enter a Algorand wallet address",
                        "list": false
                    },
                    {
                        "key": "aamt",
                        "label": "Amount",
                        "type": "string",
                        "placeholder": "",
                        "list": false
                    }
                ],
                "sample": {}
            }
        }
    ],
    "icon": "data:image/png;base64,"
}
