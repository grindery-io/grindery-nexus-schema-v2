  "actions": [
    {
      "key": "transferFlowToken",
      "name": "Transfer Flow Token",
      "display": {
        "label": "Transfer Flow Token",
        "description": "Transfer Flow Token"
      },
      "operation": {
        "type": "blockchain:call",
        "signature": "[amount: UFix64, recipient: Address]",
        "inputFields": [
          {
            "key": "_grinderyChain",
            "type": "string",
            "computed": true,
            "default": "flow:mainnet"
          },
          {
            "key": "_grinderyContractAddress",
            "type": "string",
            "computed": true,
            "default": "0x0",
            "label": "Contract Address",
            "helpText": "",
            "required": false
          },
          {
            "key": "amount",
            "label": "Amount",
            "type": "string",
            "placeholder": "0.12345678 (must have 8 digits after decimal)",
            "list": false,
            "required": true
          },
          {
            "key": "recipient",
            "label": "Recipient",
            "type": "string",
            "placeholder": "Enter recipient's Flow wallet address",
            "list": false,
            "required": true,
            "helpText": "",
            "default": "",
            "computed": false
          }
        ],
        "outputFields": [],
        "sample": {}
      }
    }
