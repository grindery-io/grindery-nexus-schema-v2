    "triggers": [],
    "type": "web2",
    "authentication": {
      "type": "oauth2",
      "authenticatedRequestTemplate": {
        "headers": {
          "Authorization": "Bearer {{ auth.access_token }}"
        }
      },
      "test": {
        "method": "GET",
        "url": "https://cds-editor.grindery.org/api/v1/contributor"
      },
      "defaultDisplayName": "{{ data.username }}",
      "allowedHosts": [
        "orchestrator.grindery.org",
        "login,grindery.io",
        "cds-editor.grindery.org"
      ],
      "oauth2Config": {
        "authorizeUrl": "https://login.grindery.io?response_type=code",
        "getAccessToken": {
          "method": "POST",
          "url": "https://orchestrator.grindery.org/oauth/token",
          "headers": {
            "Content-Type": "application/x-www-form-urlencoded"
          },
          "body": "code={{ code|urlencode }}&grant_type=authorization_code"
        },
        "autoRefresh": true,
        "refreshAccessToken": {
          "method": "POST",
          "url": "https://orchestrator.grindery.org/oauth/token",
          "headers": {
            "Content-Type": "application/x-www-form-urlencoded"
          },
          "body": "refresh_token={{ auth.refresh_token|urlencode }}&grant_type=refresh_token"
        }
      }
